{
    "mainEntryPointFilePath": "types/index.d.ts",
    "apiReport": {
        "enabled": true,
        "reportFolder": "<projectFolder>/api/"
    },
    "docModel": {
        "enabled": true,
        "apiJsonFilePath": "<projectFolder>/dist/<unscopedPackageName>.api.json"
    },
    "dtsRollup": {
